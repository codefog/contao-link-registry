{
    "name": "codefog/contao-link-registry",
    "description": "Link Registry bundle for Contao Open Source CMS",
    "keywords": ["contao", "link", "registry"],
    "type": "contao-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Codefog",
            "homepage": "https://codefog.pl"
        }
    ],
    "require": {
        "php": ">=7.1",
        "contao/core-bundle": "^4.3"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.2",
        "phpunit/phpunit": "^6.0",
        "satooshi/php-coveralls": "2.*@dev"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Codefog\\LinkRegistryBundle\\": "src",
            "Codefog\\LinkRegistryBundle\\Test\\": "tests"
        }
    },
    "extra": {
        "contao-manager-plugin": "Codefog\\LinkRegistryBundle\\ContaoManager\\Plugin"
    }
}
